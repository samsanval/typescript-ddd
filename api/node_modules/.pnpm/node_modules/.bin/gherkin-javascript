#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules/@cucumber/gherkin-streams/bin/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules/@cucumber/gherkin-streams/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules/@cucumber/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules/@cucumber/gherkin-streams/bin/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules/@cucumber/gherkin-streams/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules/@cucumber/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@26.2.0_@cucumber+message-streams@4.0.1_@cucumber+messages@22.0.0/node_modules:/Users/samuelsanchez/Documents/secture/typescript-express-dd/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cucumber/gherkin-streams/bin/gherkin" "$@"
else
  exec node  "$basedir/../@cucumber/gherkin-streams/bin/gherkin" "$@"
fi
